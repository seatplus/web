{
  "name": "seatplus/web",
  "description": "Web frontend of seatplus",
  "license": "MIT",
  "authors": [
    {
      "name": "Felix Huber",
      "email": "felix.a.huber@gmx.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seatplus\\Web\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
        "Seatplus\\Web\\Tests\\": "tests/",
        "Seatplus\\Eveapi\\": "../eveapi/src/",composer.json
        "Seatplus\\Auth\\": "../auth/src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "laravel/framework": "^6.0",
    "laravel/socialite": "^4.1",
    "inertiajs/inertia-laravel": "^0.2.4",
    "tightenco/ziggy": "^0.8.0",
    "thepinecode/i18n": "^0.7.0",
    "spatie/laravel-permission": "^3.0",
      "laravel/horizon": "^3.7",
      "predis/predis": "^1.1",
      "cakephp/chronos": "^1.3"
  },
  "require-dev": {
    "orchestra/testbench": "^4.2",
    "spatie/phpunit-watcher": "^1.11"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seatplus\\Web\\WebServiceProvider"
      ]
    }
  },
  "scripts": {
    "watch": "vendor/bin/phpunit-watcher watch"
  }
}
