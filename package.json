        "jquery": "^3.4",
        "jsdom": "^15.2.1",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^4.1.4",
        "laravel-mix-copy-watched": "^2.2.2",
        "lodash": "^4.17.5",
        "mocha": "^6.2.2",
        "mochapack": "^1.1.13",
        "popper.js": "^1.16.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.24.5",
